<p id="notice" class="w3-panel"><%= notice %></p>

<h1>User1s</h1>

<table class="w3-table w3-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user1s.each do |user1| %>
      <tr>
        <td><%= user1.name %></td>
        <td><%= user1.email %></td>
        <td><%= link_to 'Show', user1 , class:"w3-button w3-red" %></td>
        <td><%= link_to 'Edit', edit_user1_path(user1) , class:"w3-button w3-red" %></td>
        <td><%= link_to 'Destroy', user1, method: :delete, data: { confirm: 'Are you sure?' } , class:"w3-button w3-red" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User1', new_user1_path , class:"w3-button w3-red" %>
